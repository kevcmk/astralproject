{
  "name": "Astral Project",
  "version": "1.0",
  "description": "Astral project a file into Coderpad",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },

  "browser_action": {
    "default_title": "Synchronize with `target-file`"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
